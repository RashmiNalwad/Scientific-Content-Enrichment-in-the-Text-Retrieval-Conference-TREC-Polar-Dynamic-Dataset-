                    var item = list[y].href
                    if (item!=null && 0<item.length)
                    {
                        href = item;
                        break;
                    }
                }
            }
            if (href!=null && 0<href.length)
            {
                //append suffix if configured
                if (config.s!=null && 0<config.s.length){href += config.s;}
                viewPage(
                    href
                    , Math.min(window.screen.width,config.w)
                    , config.h);
            }
        }
        catch (e){return true;}
        return false;
    }
